{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*\\.(html|css|js|png|jpg|jpeg|gif|svg|ico))",
      "dest": "/public/$1"
    },
    {
      "src": "/salon(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/users(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/fav_salon(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/contact(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/register(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/config(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}